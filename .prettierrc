{
  "semi": true,
  "trailingComma": "none",
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "quoteProps": "as-needed",
  "bracketSameLine": false,
  "overrides": [
    {
      "files": "*.mjs",
      "options": {
        "parser": "babel"
      }
    }
  ]
}
